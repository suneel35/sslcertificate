        Options -Indexes +FollowSymLinks
        AllowOverride All
    </Directory>

    ErrorLog ${APACHE_LOG_DIR}/suresh.com-error.log
    CustomLog ${APACHE_LOG_DIR}/suresh.com-access.log combined
